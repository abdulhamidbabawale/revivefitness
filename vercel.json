{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "gymapp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "gymapp/wsgi.py"
    }
  ]
}
